{
	"name": "peacock",
	"productName": "Peacock",
	"version": "2.0.6",
	"description": "Open source privacy-first browser that hosts your data on the blockchain.",
	"main": "main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/Codiscite/peacock.git"
	},
	"keywords": [
		"browser",
		"web",
		"web3",
		"blockchain",
		"privacy",
		"internet"
	],
	"build": {
		"appId": "com.codiscite.peacock",
		"win": {
			"target": "nsis",
			"icon": "images/peacock.ico"
		},
		"mac": {
			"category": "books",
			"target": [
				"dmg"
			]
		},
		"nsis": {
			"perMachine": true
		}
	},
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"release": "electron-builder --publish=always",
		"dist-all": "electron-builder -mwl --publish=always"
	},
	"author": "Codiscite",
	"license": "MIT",
	"devDependencies": {
		"blockstack": "^20.0.0-alpha.4",
		"child_process": "^1.0.2",
		"cors": "^2.8.5",
		"cross-fetch": "^3.0.4",
		"discord-rpc": "^3.1.0",
		"dragula": "^3.7.2",
		"electron": "^8.0.0-beta.3",
		"electron-builder": "^20.28.4",
		"electron-tabs": "^0.9.5",
		"express": "^4.17.1",
		"gulp": "^3.9.1",
		"jsonfile": "^2.4.0",
		"uuid": "^3.0.1"
	},
	"dependencies": {
		"@cliqz/adblocker-electron": "^1.4.0",
		"async": "^3.1.0",
		"autocompleter": "^5.2.0",
		"blockstack": "^20.0.0-alpha.4",
		"child_process": "^1.0.2",
		"cors": "^2.8.5",
		"cross-fetch": "^3.0.4",
		"darkreader": "^4.7.15",
		"discord-rpc": "^3.1.0",
		"dragula": "^3.7.2",
		"electron-context-menu": "^0.15.1",
		"electron-dl": "^1.14.0",
		"electron-tabs": "^0.9.5",
		"electron-unhandled": "^3.0.1",
		"electron-util": "^0.12.3",
		"express": "^4.17.1",
		"gulp": "^3.9.1",
		"install": "^0.13.0",
		"jquery": "^3.4.1",
		"jquery-ui": "^1.12.1",
		"jquery-ui-dist": "^1.12.1",
		"jsonfile": "^2.4.0",
		"npm": "^6.13.4",
		"tldts": "^5.6.2",
		"uuid": "^3.0.1",
		"v8-compile-cache": "^2.1.0"
	}
}
